    "@emotion/styled": "11",
    "atomic-router": "0.7.1",
    "atomic-router-react": "0.7.4",
    "effector": "22.3.0",
    "effector-react": "22.1.6",
    "framer-motion": "6",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-intl": "^6.2.5",
    "vite-plugin-svgr": "2.2.1"
  },
